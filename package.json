{
  "name": "lit-ui-router-turborepo",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "ci": "turbo run ci",
    "dev": "turbo run dev",
    "format": "turbo run format",
    "format:check": "turbo run format:check",
    "format:check:root": "prettier --check \"*.{md,ts,js,json,jsonc}\"",
    "format:root": "prettier --write \"*.{md,ts,js,json,jsonc}\"",
    "lint": "turbo run lint",
    "lint:root": "eslint --format tap --ignore-pattern apps --ignore-pattern tools --ignore-pattern docs --ignore-pattern packages --ignore-pattern examples",
    "test:coverage": "turbo run test:coverage"
  },
  "engines": {
    "node": ">=22.17.1"
  },
  "packageManager": "pnpm@10.28.1",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-formatter-tap": "^9.0.1",
    "eslint-plugin-package-json": "^0.88.1",
    "eslint-plugin-turbo": "^2.7.5",
    "globals": "^17.0.0",
    "pnpm": "^10.28.1",
    "prettier": "^3.8.0",
    "turbo": "^2.7.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1",
    "wrangler": "^4.59.2"
  },
  "pnpm": {
    "overrides": {
      "d3-color": "3.1.0",
      "dompurify": "3.3.1",
      "esbuild": "0.27.2",
      "undici@6": "6.23.0",
      "undici@7": "7.18.2"
    }
  }
}
