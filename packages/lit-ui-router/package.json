{
  "name": "lit-ui-router",
  "version": "1.1.2",
  "type": "module",
  "module": "./dist/index.js",
  "homepage": "https://lit-ui-router.dev",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/simshanith/lit-ui-router"
  },
  "sideEffects": [
    "**/ui-router.js",
    "**/ui-view.js"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage --browser=chromium",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\"",
    "lint": "eslint .",
    "build:custom-elements": "cem analyze --outdir ./dist",
    "docs:api": "rimraf ../../docs/api/reference && typedoc --defaultCategory other"
  },
  "customElements": "./dist/custom-elements.json",
  "dependencies": {
    "@uirouter/core": "^6.0.8",
    "lit": "^2.0.0 || ^3.0.0"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "0.10.4",
    "@vitest/browser-playwright": "^4.0.16",
    "@vitest/coverage-v8": "^4.0.16",
    "playwright": "1.54.1",
    "prettier": "3.6.2",
    "release-it": "^19.2.2",
    "rimraf": "^6.0.0",
    "typedoc": "^0.28.15",
    "typedoc-plugin-lit-ui-router": "workspace:*",
    "typedoc-plugin-markdown": "^4.4.0",
    "typedoc-vitepress-theme": "^1.1.2",
    "typescript": "5.8.3",
    "vitest": "^4.0.16"
  }
}
