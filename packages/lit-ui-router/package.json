{
  "name": "lit-ui-router",
  "version": "0.0.1",
  "type": "module",
  "module": "./src/index.js",
  "sideEffects": [
    "**/ui-router.js",
    "**/ui-view.js"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "format": "prettier --write \"**/*.{ts,json,md}\"",
    "lint": "prettier --check \"**/*.{ts,json,md}\"",
    "build:custom-elements": "cem analyze --outdir ./dist"
  },
  "customElements": "./dist/custom-elements.json",
  "dependencies": {
    "@uirouter/core": "^6.0.8",
    "lit": "^2.0.0 || ^3.0.0"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "0.10.4",
    "@vitest/browser": "^3.2.4",
    "playwright": "1.54.1",
    "prettier": "3.6.2",
    "typescript": "5.8.3",
    "vitest": "^3.2.4"
  }
}
