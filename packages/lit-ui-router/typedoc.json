{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["src/index.ts"],
  "out": "../../docs/api/reference",
  "docsRoot": "../../docs",
  "plugin": [
    "typedoc-plugin-markdown",
    "typedoc-vitepress-theme",
    "typedoc-plugin-lit-ui-router"
  ],
  "blockTags": [
    "@author",
    "@callback",
    "@category",
    "@categoryDescription",
    "@default",
    "@deprecated",
    "@document",
    "@enum",
    "@event",
    "@eventProperty",
    "@example",
    "@extends",
    "@group",
    "@groupDescription",
    "@hidden",
    "@hideconstructor",
    "@ignore",
    "@import",
    "@inheritDoc",
    "@interface",
    "@internal",
    "@license",
    "@mergeModuleWith",
    "@module",
    "@namespace",
    "@overload",
    "@packageDocumentation",
    "@param",
    "@private",
    "@privateRemarks",
    "@property",
    "@protected",
    "@public",
    "@readonly",
    "@remarks",
    "@returns",
    "@satisfies",
    "@sealed",
    "@see",
    "@since",
    "@slot",
    "@summary",
    "@template",
    "@throws",
    "@type",
    "@typedef",
    "@typeParam",
    "@useDeclaredType",
    "@version",
    "@virtual"
  ],
  "excludeExternals": true,
  "excludeInternal": true,
  "excludePrivate": true,
  "categorizeByGroup": true,
  "categoryOrder": ["Core", "Components", "Directives", "Hooks", "Types"],
  "readme": "none",
  "githubPages": false,
  "entryFileName": "index",
  "useCodeBlocks": true,
  "expandObjects": true,
  "parametersFormat": "table",
  "propertiesFormat": "table",
  "enumMembersFormat": "table",
  "typeDeclarationFormat": "table",
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  },
  "sort": ["alphabetical", "source-order"],
  "router": "kind",
  "formatWithPrettier": true,
  "prettierConfigFile": "../../.prettierrc"
}
