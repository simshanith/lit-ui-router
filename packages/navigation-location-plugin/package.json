{
  "name": "ui-router-navigation-location-plugin",
  "description": "UI Router Location Plugin for Navigation API",
  "version": "0.1.1",
  "license": "MIT",
  "homepage": "https://github.com/simshanith/lit-ui-router/tree/main/packages/navigation-location-plugin",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simshanith/lit-ui-router.git",
    "directory": "packages/navigation-location-plugin"
  },
  "bugs": {
    "url": "https://github.com/simshanith/lit-ui-router/issues"
  },
  "keywords": [
    "ui-router",
    "navigation-api",
    "location",
    "plugin",
    "spa",
    "routing"
  ],
  "type": "module",
  "module": "./dist/index.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsc",
    "changelog": "release-it --changelog --git.tagMatch=\"ui-router-navigation-location-plugin@[0-9]*.[0-9]*.[0-9]*\"",
    "commit:changelog": "echo \"Release ${version}\n\n$(pnpm --silent changelog)\"",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\"",
    "lint": "eslint .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage --browser=chromium"
  },
  "peerDependencies": {
    "@uirouter/core": "^6.0.8"
  },
  "devDependencies": {
    "@types/dom-navigation": "^1.0.6",
    "@uirouter/core": "^6.0.8",
    "@vitest/browser-playwright": "^4.0.16",
    "@vitest/coverage-v8": "^4.0.16",
    "playwright": "1.54.1",
    "prettier": "3.6.2",
    "release-it": "^19.2.2",
    "typescript": "5.8.3",
    "vitest": "^4.0.16"
  }
}
