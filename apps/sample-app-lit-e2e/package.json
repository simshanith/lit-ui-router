{
  "name": "sample-app-lit-e2e",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "e2e": "cypress open",
    "test:cypress": "cypress run",
    "docs:start-server": "pnpm --filter docs run wrangler:dev --port 8787",
    "test": "start-server-and-test docs:start-server 'http://localhost:8787' test:cypress",
    "build": "tsc",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\"",
    "lint": "eslint ."
  },
  "dependencies": {
    "docs": "workspace:*"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.10",
    "cypress": "^14.5.2",
    "prettier": "3.6.2",
    "start-server-and-test": "^2.1.3",
    "typescript": "5.8.3",
    "vite": "^7.0.5"
  }
}
